package API;

public class Edin {

	public static void main(String[] args) {
		
		String[] valute = new String[] {"USD 1.00", "EUR 0.88457", "GBP 0.65104", "INR 63.9335", "AUD 1.30151"};
		String val = "";
		String name = "";

		for (int i = 0; i < 9; i++) {
			val = valute[i];
			String[] line = val.split(" ");
			name = line[0];
			String kurs = line[1];
			double vrKurs = Double.parseDouble(kurs);
		}
		
		
		// EUR  10   GBP
		double value;
		
		switch (name) {
			case EUR:
				value = 10/0.88457 * 0.65104;
		}

	}

}

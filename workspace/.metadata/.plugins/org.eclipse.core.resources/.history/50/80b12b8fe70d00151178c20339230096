package API;

public class Vjezbe5 {

	public static void main(String[] args) {
		
		double max = Double.parseDouble(args[0]);
		for (int i = 0; i<args.length; i++){
			double num = Double.parseDouble(args[i]);
			if (num>max){
				max = num;
			}
		} System.out.println("Biggest number is: " + max);
	}
}

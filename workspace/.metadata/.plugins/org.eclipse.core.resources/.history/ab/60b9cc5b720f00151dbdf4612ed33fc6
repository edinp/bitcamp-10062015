	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		
		System.out.println("How many rows");
		int rows = in.nextInt();
		System.out.println("How many columns");
		int columns = in.nextInt();

		int[][] nizz = new int[rows][columns];
		
		NizzHelper.fillNizz(nizz);
		NizzHelper.print2DArray(nizz);
		
		int whatColumn = 2;
		NizzHelper.getColumn(nizz, whatColumn);
		
		int[][] funnyNizz = new int[3][];
		for(int i = 0; i < funnyNizz.length; i++) {
			NizzHelper.print2DArray(funnyNizz);
		}
		
		in.close();

	}

}

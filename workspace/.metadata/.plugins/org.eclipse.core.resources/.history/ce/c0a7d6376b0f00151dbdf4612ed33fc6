package Arrays2D;

import java.util.Arrays;

public class Vjezbe1 {

	public static boolean isSingular (int[][] matrix) {
		
	}
	
	
	public static void main(String[] args) {
		

		int matrix[][] = { {1,2,3}, {4,5,6}, {7,8,9} };
		
		int matrix1[] = new int[matrix.length+2];
		
		for (int i = 0; i < matrix1.length; i++) {
			matrix1[i] = matrix[i][0];
		} System.out.println(Arrays.toString(matrix1));
				
		int num = 0;
		
		for (int i = 0; i < matrix.length; i++) {
			for (int j = 0; j < matrix[i].length; j++) {
					num=matrix[i][j] * matrix[i+1][j+1] * matrix[i+2][j+2];
			}
		}

	}

}

//		
//		for (int i = 0; i < array[i].length; i++) {
//			array[0] = 
//		
//		}
//	}

	public static void main(String[] args) {
		

		int[] niz = new int[] {1, 2, 3, 4};
		
		print1DArray(niz);
		
		
		int[][] nizz = new int[4][5];

	}

}
